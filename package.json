  "scripts": {
    "clean": "rm -rf build",
    "build:prod": "babel src --out-file lib/index.js --source-maps && npm run build:styles",
    "build:scripts": "cd ./src && NODE_ENV=production babel -d ../lib './**/*.js'",
    "build:styles": "cp ./postcss.config.js ./src/styles.scss lib/ && NODE_ENV=production node-sass < src/styles.scss > ./lib/styles.css",
    "build": "npm run clean && npm run build:scripts && npm run build:styles",
    "start": "babel src --watch --out-dir lib",
    "test": "standard && node my-tests.js"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.2",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-html-parser": "^2.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "babel-plugin-uglify": "^1.0.2",
    "babel-preset-minify": "^0.5.0",
