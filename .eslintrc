{
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "prettier"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  },
  "rules": {
    "prettier/prettier": "error",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "jsx-a11y/media-has-caption": 0,
    "jsx-a11y/click-events-have-key-events": 0,
    "no-underscore-dangle": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "import/no-duplicates": "error",
    "import/first": 0,
    "import/prefer-default-export": 0,
    "import/order": 0,
    "react/no-danger": 0,
    "react/forbid-prop-types": 0,
    "react/jsx-closing-bracket-location": 0,
    "react/prop-types": [
      2,
      {
        "skipUndeclared": true,
        "ignore": [
          "t"
        ]
      }
    ],
    "no-nested-ternary": 0,
    "comma-dangle": 0,
    "react/require-default-props": 0,
    "object-curly-newline": 0,
    "arrow-parens": [
      "error",
      "as-needed"
    ],
    "no-useless-constructor": [
      "error"
    ],
    "no-plusplus": [2, { "allowForLoopAfterthoughts": true }],
    "no-unreachable": 0,
    "consistent-return": 0,
    "react/no-array-index-key": 0,
    "camelcase": 0,
    "global-require": 0,
    "no-console": 0,
    "radix": [
      "error",
      "as-needed"
    ],
    "no-throw-literal": 0,
    "symbol-description": 0,
    "no-restricted-syntax": [
      "error",
      {
        "selector": "ForInStatement",
        "message": "for..in loops iterate over the entire prototype chain, which is virtually never what you want. Use Object.{keys,values,entries}, and iterate over the resulting array. - Airbnb"
      },
      {
        "selector": "LabeledStatement",
        "message": "Labels are a form of GOTO; using them makes code confusing and hard to maintain and understand. - Airbnb"
      },
      {
        "selector": "WithStatement",
        "message": "`with` is disallowed in strict mode because it makes code impossible to predict and optimize. - Airbnb"
      }
    ],
    "react/destructuring-assignment": 0,
    "no-shadow": 1,
    "class-methods-use-this": 0,
    "no-void": 0
  }
}